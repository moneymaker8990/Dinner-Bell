{
  "expo": {
    "name": "dinner-bell",
    "slug": "dinner-bell",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dinnerbell",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F1115",
      "dark": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#0F1115"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dinnerbell.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSContactsUsageDescription": "Dinner Bell uses your contacts to let you invite guests to events by phone number.",
        "NSPhotoLibraryUsageDescription": "Dinner Bell uses your photos to set event cover images and profile pictures.",
        "NSCalendarsUsageDescription": "Dinner Bell uses your calendar to add dinner events."
      }
    },
    "android": {
      "package": "com.dinnerbell.app",
      "permissions": [
        "READ_CONTACTS",
        "READ_MEDIA_IMAGES",
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0F1115"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
